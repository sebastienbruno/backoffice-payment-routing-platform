{
  "name": "message-ingest-service",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "chmod +x mvnw && ./mvnw clean package -DskipTests",
        "cwd": "apps/message-ingest-service"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "chmod +x mvnw && ./mvnw spring-boot:run",
        "cwd": "apps/message-ingest-service"
      }
    }
  }
}
